{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"declaration": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": ["es2018", "DOM"],
		"moduleResolution": "node",
		"noEmit": false,
		"outDir": "lib",
		"strict": true,
		"target": "esnext"
	},
	"include": ["./src"],
	"exclude": [
		"jquery.ajax.js",
		"rest.js"
	]
}